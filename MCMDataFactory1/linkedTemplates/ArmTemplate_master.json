{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MCMDataFactory1"
		},
		"CRM UAT Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CRM UAT Blob'"
		},
		"LocalBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LocalBlob'"
		},
		"PSFTRestAPI_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'PSFTRestAPI'"
		},
		"SqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer'"
		},
		"McMasterAPIs_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://apis.mcmaster.ca/mcmaster-university/sandbox/holidays/MACUG/UGRD/2201"
		},
		"PSFTRestAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://hrpre.mcmaster.ca:8000/PSIGW/RESTListeningConnector/PSFT_HR/ExecuteQuery.v1/PUBLIC/MCM_HR_POSITION_DATA_DBT/JSON/NONFILE?isconnectedquery=n&maxrows=3"
		},
		"PSFTRestAPI_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "PHELANC"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MCMDataFactory1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CRM UAT Blob_connectionString": {
						"value": "[parameters('CRM UAT Blob_connectionString')]"
					},
					"LocalBlob_connectionString": {
						"value": "[parameters('LocalBlob_connectionString')]"
					},
					"PSFTRestAPI_password": {
						"value": "[parameters('PSFTRestAPI_password')]"
					},
					"SqlServer_connectionString": {
						"value": "[parameters('SqlServer_connectionString')]"
					},
					"McMasterAPIs_properties_typeProperties_url": {
						"value": "[parameters('McMasterAPIs_properties_typeProperties_url')]"
					},
					"PSFTRestAPI_properties_typeProperties_url": {
						"value": "[parameters('PSFTRestAPI_properties_typeProperties_url')]"
					},
					"PSFTRestAPI_properties_typeProperties_userName": {
						"value": "[parameters('PSFTRestAPI_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}